    <script>
       @*var urlObtenerEntidad = '@Url.Action("ObtenerEntidad", "FichaCliente")';
       var urlGuardarFicha = '@Url.Action("GuardarFicha", "FichaCliente")';
       var urlObtenerRegion = '@Url.Action("ObtenerRegion", "FichaCliente")';
       var urlObtenerEstadoFicha = '@Url.Action("ObtenerEstadoFicha", "FichaCliente")';
       var urlObtenerTipoContratacion = '@Url.Action("ObtenerTipoContratacion", "FichaCliente")';
       var urlObtenerProveedor = '@Url.Action("ObtenerProveedor", "FichaCliente")';
       var urlAgregarEstablecimiento = '@Url.Action("AgregarEstablecimiento", "FichaCliente")';
       var urlEliminarFicha = '@Url.Action("EliminarFicha", "FichaCliente")';*@



        $(document).ready(function () {

            $('.ui.selection.dropdown').dropdown();
            $('.ui.menu .ui.dropdown').dropdown({
                on: 'hover'
            });

            $('.ui.accordion')
                .accordion()
                ;
        });


    </script>
</head>
<body>
    <div class="ui link cards">
        <div class="container">
            <div style="height:5px"></div>
            <h3 style="color:white" class="ui center aligned header">
                <img src="~/Recursos/Imagenes/LogoBackline.png" class="ui image">
                Bienvenido al area de consultas de soporte Backline @Model.NombreUsuario
            </h3>
            <div class="row">
                <div class="col-md-4">

                    <div class="card mb-4 shadow-sm" style="opacity:0.9">
                        <h4 style="color: black" class="ui left aligned header">
                            <img src="~/Recursos/Imagenes/LogoBackline.png" class="ui image">
                            Sistema de farmacias
                        </h4>
                        <div class="contenedor-imagen">
                            <img class="card-img-top" src="~/Recursos/Imagenes/Consultas.jpg" alt="Card image cap">
                        </div>
                        <div class="card-body">
                            <p class="card-text">Aquí podrás consultar los usuarios y movimientos de cada farmacia.</p>
                            <div class="d-flex justify-content-between align-items-center">
                                <div class="btn-group">
                                    <div id="btnMarketPlace" onclick="window.location.href = '/ConsultasFarmacias'" class="ui right labeled icon black button">
                                        Ingresar a consultas de farmacia
                                        <i class="hand point left icon"></i>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                @*Solicitudes*@
                <div class="col-md-4">
                    <div class="card mb-4 shadow-sm" style="opacity: 0.9">
                        <h4 style="color: black" class="ui left aligned header">
                            <img src="~/Recursos/Imagenes/LogoBackline.png" class="ui image">
                            Sistema de droguería
                        </h4>
                        <div class="contenedor-imagen">
                            <img class="card-img-top" src="~/Recursos/Imagenes/Consultas.jpg" alt="Card image cap">
                        </div>
                        <div class="card-body">
                            <p class="card-text">Aquí podrás consultar los usuarios y movimientos de cada droguería.</p>
                            <div class="d-flex justify-content-between align-items-center">
                                <h4 style="color: black" class="ui left aligned header">
                                    Próximamente!!
                                </h4>
                                <div class="btn-group">
                                    <div id="btnMarketPlace" onclick="window.location.href = '/ConsultasDrogueria'" class="ui right labeled icon black button" style="display:none">
                                        Ingresar a consultas de drogueria
                                        <i class="hand point left icon"></i>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="card mb-4 shadow-sm" style="opacity: 0.9">
                        <h4 style="color: black" class="ui left aligned header">
                            <img src="~/Recursos/Imagenes/LogoBackline.png" class="ui image">
                            Sistema de venta simple
                        </h4>
                        <div class="contenedor-imagen">
                            <img class="card-img-top" src="~/Recursos/Imagenes/Consultas.jpg" alt="Card image cap">
                        </div>
                        <div class="card-body">
                            <p class="card-text">Aquí podrás consultar los usuarios y movimientos de cada punto de venta simple.</p>
                            <div class="d-flex justify-content-between align-items-center">
                                <h4 style="color: black" class="ui left aligned header">
                                    Próximamente!!
                                </h4>
                                <div class="btn-group">
                                    <div id="btnMarketPlace" onclick="window.location.href = '/ConsultasPuntoVentaSimple'" class="ui right labeled icon black button" style="display:none">
                                        Ingresar a consultas de venta simple
                                        <i class="hand point left icon"></i>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
